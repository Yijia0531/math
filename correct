      <Sentence>
        <logic>$\vee$(p,q)</logic>
        <logic>FalseProposition(p)</logic>
      </Sentence>
      <Sentence>
        <logic>$\wedge$(p,q)</logic>
        <logic>TrueProposition(p)</logic>
      </Sentence>
      <Sentence>
        <logic>Asking(rs_f)</logic>
        <logic>ValueRangeOfParameter(k,rs_f)</logic>
      </Sentence>

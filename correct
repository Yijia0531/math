        <logic>(Perpendicular(m,$\alpha$)):-(Exist($\alpha$))</logic>
        <logic>Plane($\alpha$)</logic>
      </Sentence>
      <Sentence>
        <logic>Perpendicular(n,$\alpha$)</logic>
      </Sentence>
    </option>
    <option value="B">
      <Sentence>

<questions id="questions_04" score="12">  <text format="latex">
    <Sentence>
      <logic>Proposition(p,(Function(rs_e,$y=kx+1$),IncreasingFunctionOn(rs_e,R),Proposition(q,(Curve(rs_a,$y=x^{2}+(2k-3)x+1$),Intersect(rs_a,rs_b,rs_c),Intersect(rs_a,rs_b,rs_d),AreDifferent(rs_c,rs_d),X-axis(rs_b),Point(rs_c),Point(rs_d)))))</logic>

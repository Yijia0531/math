<question id="2012nianquanguokaoshizhejiangjuanshuexuewenke_5" score="5" type="select">  <text format="latex">
    <Sentence>
      <logic>Line(l)</logic>
    </Sentence>
    <Sentence>
      <logic>Plane($\beta$)</logic>
      <logic>Asking1($\beta$)</logic>
      <logic>Plane($\alpha$)</logic>
      <logic>Asking1($\alpha$)</logic>
      <logic>AreDifferent($\beta$,$\alpha$)</logic>
    </Sentence>
  </text>
  <select format="latex" multiple="false">
    <option value="A">
      <Sentence>

      <logic>Function(f(x),$\log_{a}{(x-1)}$)</logic>
      <logic>Function(g(x),$\log_{a}(4-2x)(a$)</logic>
      <logic>GreaterThan(f(x),0)</logic>
      <logic>GreaterThan(g(x),0)</logic>
    </Sentence>
    <Sentence>
      <logic>NoEqual(a,1)</logic>
    </Sentence>
  </text>
